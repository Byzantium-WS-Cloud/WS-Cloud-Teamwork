<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://byzantium-socialnetwork.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>byzantium-socialnetwork.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>byzantium-socialnetwork</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>False</EnableMSDeployBackup>
    <UserName>$byzantium-socialnetwork</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="FacebookDbConnection" Order="1" Enabled="False">
          <Destination Path="Data Source=tcp:byzantium-db.database.windows.net,1433;Initial Catalog=byzantium-socialnetwork_db;User ID=dbadmin@byzantium-db;Password=Byzantium123" Name="Data Source=tcp:byzantium-db.database.windows.net,1433;Initial Catalog=byzantium-socialnetwork_db;User Id=dbadmin@byzantium-db;Password=Byzantium123" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=.;Initial Catalog=FaceBookSystem;Integrated Security=True" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\FacebookDbConnection_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Data Source=.;Initial Catalog=FaceBookSystem;Integrated Security=true" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)FacebookDbConnection-Web.config Connection String">
      <ParameterValue>Data Source=tcp:byzantium-db.database.windows.net,1433;Initial Catalog=byzantium-socialnetwork_db;User Id=dbadmin@byzantium-db;Password=Byzantium123</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>